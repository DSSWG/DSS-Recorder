{
  "name": "DSS Recorder",
  "version": "0.0.1",
  "description": "Record interactions for your UI components",
  "manifest_version": 2,
  "icons": {
  },
  "browser_action": {
    "default_title": "Record an interaction",
    "default_icon": {
      "19": "images/ico-19.png",
      "38": "images/ico-38.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ]
}
